{
  "name": "vue-starport",
  "version": "0.0.1",
  "packageManager": "pnpm@6.32.3",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "pkgroll",
    "dev": "pkgroll --watch",
    "play": "vite playground --open",
    "play:build": "vite build playground",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --tag --push && pnpm publish"
  },
  "dependencies": {
    "@vueuse/core": "^8.2.4",
    "nanoid": "^3.3.2",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.19.4",
    "@antfu/ni": "^0.14.0",
    "@types/node": "^17.0.23",
    "bumpp": "^7.1.1",
    "eslint": "^8.12.0",
    "pkgroll": "^1.0.3",
    "pnpm": "^6.32.4",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vitest": "^0.8.2",
    "vue-tsc": "^0.33.9"
  }
}
